<h1>Picgur (<%= @pictures.length %> pictures)</h1>
<p>
  <%= link_to "post a picture", new_picture_path %>
</p>

<% @pictures.each do |picture| %>
  <%= link_to image_tag(picture.url), "/pictures/#{picture.id}" %>
  <p> <span style="color: blue;">@<%= picture.artist %></span> <%= picture.title %></p>
  <br>
  <br>
  <br>
<% end %>
